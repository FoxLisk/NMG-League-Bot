{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"args": [
				"--features",
				"racetime_bot,testing,development"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: cargo build"
		},
		{
			"type": "shell",
			"command": "npm run build:watch",
			"label": "build statics",
			"runOptions": {
				"runOn": "folderOpen"
			},
			"problemMatcher": []
		}
	]
}